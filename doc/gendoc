#!/bin/bash

function extract 
{
    sed -n '/^\/\/\/ /s,/// ,,p' ../fit/$1.h > src/$1.md
}

extract always
extract args
extract compose
extract conditional
extract fix
extract fuse
extract identity
extract implicit
extract indirect
extract infix
extract invoke
extract is_callable
extract lambda
extract lazy
extract match
extract mutable
extract by
extract partial
extract pipable
extract placeholders
extract protect
extract returns
extract reveal
extract static
extract tap
extract variadic
